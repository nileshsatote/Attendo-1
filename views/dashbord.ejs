    <style>
      html {
    background-color: #E4E9FD;
    background-image: -webkit-linear-gradient(65deg, #A683E3 50%, #E4E9FD 50%);
    min-height: 1000px;
    font-family: 'helvetica neue';
  }

      .hide{
        display: none;
      }
      
      </style>
</head>
<script defer src="face-api.min.js"></script>
<script defer src="dashbord.js"></script>

<body>
    <div id="lodingMassage"></div>

    <div class=" mt-5 form_cnt">
        <form class="row gx-3 gy-2 align-items-center" id="formId" >
          <div class="col-sm-3">
            <label class="visually-hidden" for="specificSizeSelect">Year</label>
            <select name="year" class="form-select" id="specificSizeSelect">
              <option value="1" selected >BTech-1st year</option>
              <option value="2">BTech-2nd year</option>
              <option value="3">BTech-3rd year</option>
              <option value="4">BTech-4rth year</option>
            </select>
          </div>

          <div class="col-sm-3">
            <label class="visually-hidden" for="specificSizeSelect">Preference</label>
            <select name="branch" class="form-select" id="specificSizeSelect">
              <option value="CS" selected >Computer Science</option>
              <option value="IT">Information Technology</option>
              <option value="CH">Chemical Engineering</option>
              <option value="CI">Civil Engineering</option>
              <option value="ME">Mechenical Engineering</option>
              <option value="EC">Electronic and Communication</option>
              <option value="AI">AI</option>
              <option value="ML">ML</option>
            </select>
          </div>

          <div class="col-sm-3">
            <label class="visually-hidden" for="specificSizeSelect">Preference</label>
            <select name="class" class="form-select" id="specificSizeSelect">
              <option value="1" selected>A1</option>
              <option value="2">A2</option>
              <option value="3">A3</option>
             
            </select>
          </div>
          <div class="col-auto">
            <button type="submit" id="submitForDetection" class="btn btn-primary">Submit</button>
          </div>
        </form>
</div>
</body>
</html>